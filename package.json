{
  "name": "wolaro2",
  "version": "1.0.0",
  "description": "Advanced Discord bot with multi-tenant architecture, AI, RPG, tickets, and giveaways",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "node -e \"console.log('âœ… Installation completed successfully!')\"",
    "dev": "ts-node src/index.ts",
    "build": "tsc --noEmitOnError false || true",
    "start": "node dist/index.js",
    "start:cluster": "node dist/cluster.js",
    "update": "bash scripts/update.sh",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop wolaro",
    "pm2:restart": "pm2 restart wolaro",
    "pm2:logs": "pm2 logs wolaro",
    "pm2:monit": "pm2 monit",
    "pm2:delete": "pm2 delete wolaro",
    "docker:build": "docker build -t wolaro:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f bot",
    "docker:restart": "docker-compose restart bot",
    "migrate": "bash scripts/migrate.sh",
    "setup": "bash scripts/setup.sh",
    "backup": "bash scripts/backup.sh",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "discord",
    "bot",
    "multi-tenant",
    "ai",
    "gemini",
    "rpg",
    "moderation",
    "economy",
    "tickets",
    "giveaways"
  ],
  "author": "theo7791l",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.16.3",
    "@discordjs/voice": "^0.17.0",
    "@discordjs/opus": "^0.9.0",
    "@google/generative-ai": "^0.21.0",
    "canvas": "^2.11.2",
    "play-dl": "^1.9.7",
    "ffmpeg-static": "^5.2.0",
    "spotify-web-api-node": "^5.0.2",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "express": "^4.21.2",
    "ws": "^8.18.0",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "rate-limiter-flexible": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/express": "^5.0.0",
    "@types/ws": "^8.5.13",
    "@types/pg": "^8.11.10",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/spotify-web-api-node": "^5.0.11",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "pm2": "^5.4.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
